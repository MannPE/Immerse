{
  "name": "@stencil/stencil-site",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Site",
  "scripts": {
    "docs": "gulp",
    "build": "gulp && stencil build --prerender && npm run host-to-firebase",
    "stdocs": "stencil build --docs",
    "deploy": "npm run build && firebase deploy",
    "host-to-firebase": "node scripts/host-to-firebase.js",
    "start": "gulp && stencil build --dev --watch --serve",
    "doc": "typedoc --options typedoc.json"
  },
  "dependencies": {
    "jmdict-parser": "^2.1.6",
    "jsdoc": "^3.6.3",
    "typedoc": "^0.15.0"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.0",
    "@stencil/dev-server": "latest",
    "@stencil/router": "^1.0.0",
    "@stencil/sass": "^1.0.0",
    "@stencil/state-tunnel": "^1.0.0",
    "@stencil/utils": "latest",
    "@types/chrome": "0.0.74",
    "gh-pages": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-markdown": "^1.2.0",
    "highlight.js": "^9.15.10",
    "workbox-build": "4.3.1"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-site.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/stencil"
  },
  "homepage": "https://github.com/ionic-team/stencil"
}
